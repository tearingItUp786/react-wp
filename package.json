{
  "name": "react-wp",
  "version": "1.0.0",
  "description": "A test react app I'm doing for fun",
  "main": "webpack.config.js",
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server",
    "format": "prettier --write --single-quote --print-width=120 --parser=flow --tab-width=2 \"js/**/*.{js,jsx}\"",
    "lint": "eslint **/*.{js,jsx} --quiet",
    "lint:fix": "eslint **/*.{js,jsx} --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "watch": "webpack --watch",
    "flow": "flow"
  },
  "pre-commit": [
    "lint",
    "format"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tearingItUp786/react-wp.git"
  },
  "author": "Taran Bains",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tearingItUp786/react-wp/issues"
  },
  "homepage": "https://github.com/tearingItUp786/react-wp#readme",
  "jest": {
    "snapshotSerializers": [
      "jest-serializer-enzyme"
    ]
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-register": "^6.26.0",
    "bootstrap": "^4.0.0",
    "cors": "^2.8.4",
    "flow-typed": "^2.3.0",
    "global": "^4.3.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-react-flow-props-to-prop-types": "^0.15.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
